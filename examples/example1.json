{
  "id": "Example 1",
  "tables": [
    {
      "id": "Source table",
      "function": "pandas:read_csv",
      "inputs": [],
      "model": {
        "filepath_or_buffer": "../examples/^GSPC.csv",
        "nrows": 1000
      },

      "column_filter": {"exclude": ["Date"]}
    },
    {
      "id": "Derived columns",

      "columns": [
        {
          "id": "diff_high_low",
          "function": "example1:diff_fn",
          "inputs": ["High", "Low"]
        },
        {
          "id": "diff_close_open",
          "function": "example1:diff_fn",
          "inputs": ["Close", "Open"]
        }
      ],

      "column_filter": {"exclude": ["Close"]}
    },
    {
      "id": "Moving averages",

      "columns": [
        {
          "id": "mean_Diff_high_low",
          "function": "numpy.core.fromnumeric:mean",
          "inputs": ["diff_high_low"],
          "extensions": [
            {"scope": "10"},
            {"scope": "5"},
            {"scope": "2"}
          ]
        },
        {
          "id": "mean_Diff_close_open",
          "function": "numpy.core.fromnumeric:mean",
          "inputs": ["diff_high_low"],
          "extensions": [
            {"scope": "10"},
            {"scope": "5"},
            {"scope": "2"}
          ]
        },
        {
          "id": "mean_Volume",
          "function": "numpy.core.fromnumeric:mean",
          "inputs": ["diff_close_open"],
          "extensions": [
            {"scope": "10"},
            {"scope": "5"},
            {"scope": "2"}
          ]
        }
      ]
    },
    {
      "id": "Analytic features",

      "columns": [
        {
          "id": "trend_Close",
          "function": "example1:linear_trend_fn",
          "inputs": ["Adj Close"],
          "extensions": [
            {"scope": "20"},
            {"scope": "10"},
            {"scope": "5"}
          ]
        },
        {
          "id": "Goal",
          "function": "pandas.core.frame:DataFrame.shift",
          "scope": "all",
          "inputs": ["Adj Close"],
          "model": {"periods": -1}
        }
      ],

      "row_filter": {"dropna": true}
    },
    {
      "id": "Prediction",

      "columns": [
        {
          "id": "Predict_Goal",
          "function": "example1:gb_predict",
          "scope": "all",
          "inputs": {"exclude": ["Goal"]},
          "train": {
            "function": "example1:gb_fit",
            "model": {"n_estimators": 500, "max_depth": 4, "min_samples_split": 2, "learning_rate": 0.01, "loss": "ls"},
            "outputs": ["Goal"]
          }
        }
      ]
    },
    {
      "id": "Sink table",
      "function": "pandas:DataFrame.to_csv",
      "inputs": ["Analytic features"],
      "model": {
        "path_or_buf": "../examples/^GSPC-output.csv",
        "index": false
      }
    }
  ]
}
