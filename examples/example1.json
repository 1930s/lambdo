{
  "id": "Example 1",
  "tables": [
    {
      "id": "Source table",
      "function": "pandas:read_csv",
      "inputs": [],
      "model": {
        "filepath_or_buffer": "../examples/^GSPC.csv",
        "nrows": 1000
      }
    },
    {
      "id": "Moving averages",

      "columns": [
        {
          "id": "mean_Volume",
          "function": "numpy.core.fromnumeric:mean",
          "inputs": ["Volume"],
          "extensions": [
            {"scope": "10"},
            {"scope": "5"},
            {"scope": "2"}
          ]
        }
      ]
    },
    {
      "id": "Sink table",
      "function": "pandas:DataFrame.to_csv",
      "inputs": ["Moving averages"],
      "model": {
        "path_or_buf": "../examples/^GSPC-output.csv",
        "index": false
      }
    }
  ]
}
